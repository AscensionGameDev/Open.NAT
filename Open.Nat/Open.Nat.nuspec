<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
    <metadata>
        <id>AscensionGameDev.Open.NAT</id>
        <version>$version$</version>
        <title>AscensionGameDev.Open.NAT</title>
        <authors>Ascension Game Dev, Alan McGovern, Ben Motmans, Lucas Ontivero</authors>
        <license type="expression">MIT</license>
        <repository type="git" url="https://github.com/AscensionGameDev/Open.NAT.git" branch="master" />
        <projectUrl>https://github.com/AscensionGameDev/Open.NAT</projectUrl>
        <icon>assets\icons\256.jpg</icon>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>
            Open.NAT is a lightweight and easy-to-use class library to allow port forwarding in NAT devices (Network Address Translator) that support Universal Plug and Play (UPNP) and/or Port Mapping Protocol (PMP).
        </description>
        <summary>Library to allow port forwarding in NAT devices that support UPNP and/or PMP.</summary>
        <language>en-US</language>
        <copyright>Please see LICENSE for more details.</copyright>
        <tags>NAT TRAVERSAL UPNP PMP PORT FORWARD .NET MONO</tags>
        <releaseNotes>
            Please look at https://github.com/AscensionGameDev/Open.NAT/blob/master/CHANGELOG.md
        </releaseNotes>
        <dependencies>
            <group targetFramework="net35">
                <dependency id="TaskParallelLibrary" version="1.0.2856.0" />
            </group>
            <group targetFramework="net45">
                <dependency id="TaskParallelLibrary" version="1.0.2856.0" />
            </group>
        </dependencies>
    </metadata>
    <files>
        <file src="bin\Release\net35\*.*" target="lib\net35" />
        <file src="bin\Release\net45\*.*" target="lib\net45" />
        <file src="..\assets\**\*.*" target="assets" />
    </files>
</package>
